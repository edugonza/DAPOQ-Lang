def T1 = 527292000000
def T2 = 1480531444303
def P1 = createPeriod(T1,T2);

casesOf(
	eventsOf(
		versionsOf(
			allClasses().where {name == "CUSTOMER"}
		).where
		{
			changed([at: "BIRTH_DATE", from:"03-DEC-49", to:"30-NOV-55"])
		}
	).where
	{
		def P2 = this.createPeriod(it.timestamp)
		this.during(P2,P1)
	}
)